{
  "name": "git-visit",
  "version": "1.6.0",
  "description": "Git command line wrapping library to git visit commit history",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --recursive -R spec",
    "testdev": "cross-env NODE_ENV=test mocha --recursive -R spec -g endtoendtest -i",
    "cover": "cross-env NODE_ENV=test istanbul cover ./node_modules/mocha/bin/_mocha --report html -- --recursive -R spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saintedlama/git-visit.git"
  },
  "author": "Christoph Walcher <christoph.walcher@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saintedlama/git-visit/issues"
  },
  "homepage": "https://github.com/saintedlama/git-visit",
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "debug": "^2.1.3",
    "string": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.9",
    "cross-env": "^2.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "rimraf": "^2.4.3",
    "shelljs": "^0.6.0"
  }
}
