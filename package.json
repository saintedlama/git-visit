{
  "name": "git-visit",
  "version": "4.1.0",
  "description": "Git command line wrapping library to git visit commit history",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=text-summary mocha --recursive --throw-deprecation",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saintedlama/git-visit.git"
  },
  "author": "Christoph Walcher <christoph.walcher@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saintedlama/git-visit/issues"
  },
  "homepage": "https://github.com/saintedlama/git-visit",
  "dependencies": {
    "debug": "^3.1.0",
    "diff2html": "^2.3.3",
    "fs-extra": "^6.0.1",
    "opted": "^1.0.2",
    "trim-left": "^1.0.1",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "file-url": "^2.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "standard-version": "^4.4.0"
  },
  "snyk": true
}
